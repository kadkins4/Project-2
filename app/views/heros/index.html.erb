<h3>All Heroes</h3>
<%= link_to "Home", teams_path %>
<%= link_to "Create Hero", new_hero_path %>
<div class="hero-pool">

  <% @heros.each do |hero| %>
  <a href="heros/<%= hero.id %>">
    <div class="hero">
      <img src="<%= hero.image %>">
      <p><%= hero.name %></p>
    </div>
  </a>
  <% end %>

</div>
